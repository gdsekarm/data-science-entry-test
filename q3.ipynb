   "source": [
    "def update_dictionary(dct, key, value):\n",
    "    if key in dct:\n",
    "        print(f\"Key '{key}' already exists. Original value: {dct[key]}\")\n",
    " \n",
    "    dct[key] = value\n",
    "\n",
    "    return dct"
   ]
