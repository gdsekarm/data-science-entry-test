    "def string_reverse(s1):\n",
    "    if type(s1) is not str:\n",
    "        print(\"Error: The input 's1' must be a string.\")\n",
    "        return None\n",
    "\n",
    "    reversed_s1 = s1[::-1]\n",
    "    \n",
    "    return reversed_s1"
   ]
